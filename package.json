{
  "main": "./dist/src/app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/src/app.js",
    "dev": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "dev:watch": "nodemon",
    "build": "tsc",
    "migrate": "ts-node src/migrate.ts",
    "start-dev": "npm run dev:watch",
    "test:api": "jest --forceExit --detectOpenHandles --testPathPattern=tests/api",
    "task-start-scanner": "ts-node ./src/tasks/scanners.task.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/passport-jwt": "^3.0.9",
    "jest": "^29.6.2",
    "prettier": "3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "routing-controllers": "^0.10.4",
    "sequelize": "^5.22.5",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^1.1.0",
    "supertest": "^6.3.3",
    "supertest-prefix": "^1.0.2",
    "terser": "^5.24.0",
    "ts-node": "^10.9.1",
    "umzug": "^2.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "tests"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  }
}
